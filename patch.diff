 seqcrud/routes/index.js | 21 +++++++++++++++++++--
 1 file changed, 19 insertions(+), 2 deletions(-)

diff --git a/seqcrud/routes/index.js b/seqcrud/routes/index.js
index 9d21ab5..a78147e 100644
--- a/seqcrud/routes/index.js
+++ b/seqcrud/routes/index.js
@@ -1,5 +1,6 @@
-var express = require('express');
-var router = express.Router();
+const express = require('express');
+const models = require('../models');
+const router = express.Router();
 
 /* GET home page. */
 router.get('/', function(req, res, next) {
@@ -10,4 +11,20 @@ router.get('/show', function(req, res, next) {
     res.render('show');
 })
 
+router.post('/create', function(req, res, next) {
+    let body = req.body;
+
+    models.post.create({
+        title: body.inputTitle,
+        writer: body.inputWriter
+    })
+    .then(result => {
+        console.log('데이터 추가 완료');
+        res.redirect('/show');
+    }).catch(err=>{
+        console.log('데이터 추가 실패');
+        console.log(err);
+    })
+})
+
 module.exports = router;
